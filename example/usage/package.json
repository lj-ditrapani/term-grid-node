{
  "name": "usage-term-grid-example",
  "version": "0.0.1",
  "description": "Simplest term-grid-ui example usage",
  "scripts": {
    "start": "node -r ts-node/register usage.ts",
    "fmt": "prettier --parser typescript --write \"*.ts\" \"src/**/*.ts\" \"test/**/*.ts\"",
    "fmtjs": "prettier --write \"src/**/*.js\" \"test/**/*.js\" \"src/**/*.json\" \"test/**/*.json\" \"*.json\"",
    "tlint": "tslint --fix -c tslint.json '*.ts' 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "npm run tlint && npm run fmt && npm run fmtjs",
    "all": "npm run lint && npm start"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**"
    ]
  },
  "keywords": [
    "todo",
    "terminal"
  ],
  "author": "Jonathan Di Trapani",
  "repository": "https://github.com/lj-ditrapani/term-grid-node",
  "license": "ISC",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.13",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.0"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@types/node": "^12.7.2",
    "term-grid-ui": "^0.5.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  }
}
