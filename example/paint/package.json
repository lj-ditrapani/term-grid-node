{
  "name": "paint-term-grid-example",
  "version": "0.0.1",
  "description": "Example paint app using term-grid-ui",
  "scripts": {
    "start": "node -r ts-node/register paint.ts",
    "fmt": "prettier --parser typescript --write \"*.ts\"",
    "tlint": "tslint --fix -c tslint.json '*.ts'",
    "lint": "npm run tlint && npm run fmt",
    "all": "npm run lint && npm start"
  },
  "keywords": [
    "paint",
    "terminal"
  ],
  "author": "Jonathan Di Trapani",
  "repository": "https://github.com/lj-ditrapani/term-grid-node",
  "license": "ISC",
  "devDependencies": {
    "prettier": "^2.0.1",
    "source-map-support": "^0.5.16",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.20.35"
  },
  "dependencies": {
    "@types/node": "^14.14.41",
    "term-grid-ui": "^0.6.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
